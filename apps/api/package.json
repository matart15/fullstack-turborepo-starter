{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "open": "open http://localhost:5000/graphql",
    "dev": "NODE_ENV=development nest start --watch",
    "dev_1": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "dev:debug": "NODE_ENV=development nest start --watch --debug",
    "start:prod": "node dist/main",
    "lint:fix": "TIMING=1 eslint . --ext .js,.ts --fix",
    "tsc": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test:server": "NODE_ENV=test dotenv -e .env.test -- nest start",
    "stg": "NODE_ENV=production nest start",
    "prod": "NODE_ENV=production node dist/src/main",
    "prisma:generate": "prisma generate",
    "db:migrate": "prisma migrate dev && prisma generate",
    "db:seed": "prisma db seed",
    "db:pull": "prisma db pull",
    "db:reset": "prisma migrate reset --force dev",
    "db:deploy": "prisma migrate deploy",
    "db:status": "prisma migrate status",
    "console": "nest start --entryFile repl",
    "db:migrate:test": "dotenv -e .env.test yarn db:migrate",
    "db:seed:test": "dotenv -e .env.test yarn db:seed",
    "db:test:reset": "dotenv -e .env.test yarn db:reset",
    "jest": "dotenv -e .env -- jest --runInBand --verbose --coverage",
    "e2e": "dotenv -e .env.test cross-env NODE_ENV=test -- jest --config ./jest-e2e.json --verbose --runInBand",
    "generate:repository": "hygen repository_generator new",
    "generate:command_handler": "hygen command_handler new",
    "test": "jest",
    "test:watch": "jest --watch --runInBand",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "test:ci": "jest --ci --runInBand"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "1.8.1",
    "@nestjs/apollo": "10.2.0",
    "@nestjs/common": "9.3.5",
    "@nestjs/config": "2.3.0",
    "@nestjs/core": "9.3.5",
    "@nestjs/graphql": "10.2.0",
    "@nestjs/jwt": "10.0.1",
    "@nestjs/passport": "9.0.1",
    "@nestjs/platform-express": "9.3.5",
    "@nestjs/swagger": "6.2.1",
    "@prisma/client": "4.10.0",
    "apollo-server-express": "3.11.1",
    "axios": "1.3.2",
    "bcrypt": "5.1.0",
    "class-validator": "0.14.0",
    "graphql": "16.6.0",
    "hexoid": "1.0.0",
    "joi": "17.7.0",
    "nanoid": "4.0.1",
    "nestjs-i18n": "10.2.6",
    "nestjs-pino": "3.1.2",
    "nestjs-prisma": "0.20.0",
    "nestjs-request-context": "2.1.0",
    "nestjs-stripe": "1.0.0",
    "nodemailer": "6.9.1",
    "oxide.ts": "1.1.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pino-http": "8.3.3",
    "pino-pretty": "9.1.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.1.2",
    "rxjs": "7.8.0",
    "stripe": "11.9.1",
    "swagger-ui-express": "4.6.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.2.0",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.5",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.4.0",
    "@types/node": "18.13.0",
    "@types/passport-jwt": "3.0.8",
    "@types/passport-local": "1.0.35",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "class-transformer": "0.5.1",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "dotenv-cli": "7.0.0",
    "eslint": "8.33.0",
    "eslint-config-custom": "0.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "hygen": "6.2.11",
    "jest": "29.4.2",
    "prettier": "2.8.4",
    "prisma": "4.10.0",
    "prisma-nestjs-graphql": "17.1.0",
    "rimraf": "4.1.2",
    "run-script-webpack-plugin": "0.1.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig": "7.0.0",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5",
    "webpack": "5.75.0",
    "webpack-node-externals": "3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": ".",
    "moduleNameMapper": {
      "prisma(.*)$": "<rootDir>/../prisma/$1"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
